{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.webflow.io https://*.webflow.com https://ntro.org.au https://www.ntro.org.au"
        }
      ]
    }
  ]
}
